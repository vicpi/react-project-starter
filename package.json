{
  "name": "react-redux-start-project",
  "description": "Example React Redux Application",
  "author": "Viktor Pyskunov <vpyskunov@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "react-router",
    "webpack",
    "boilerplate",
    "starter",
    "example",
    "hot reload",
    "server side rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vicpi/react-redux-start-project.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.dev.config.js --hot --inline --progress --history-api-fallback",
    "production": "npm run production:build && sudo npm run production:start",
    "e2e": "mocha tests/.setup.js tests/e2e/**/*-e2e.js",
    "test": "mocha tests/.setup.js tests/unit/**/*-test.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- test/.setup.js test/**/*-test.js",
    "lint": "eslint config src/js/**/*.js",
    "production:build": "NODE_ENV=production webpack -p --config ./webpack/webpack.prod.config.js --progress",
    "production:start": "NODE_PATH=$NODE_PATH:./src/js/ NODE_ENV=production node --harmony ./src/js --config ./webpack/webpack.prod.config.js",
    "clean:build": "rm -rf build && mkdir build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "rm -rf build",
    "prebuild": "npm run clean:build && npm run build:html",
    "export": "npm run export-clean && npm run export-compile && npm run export-run",
    "export-clean": "rm -rf export/build",
    "export-compile": "NODE_ENV=production webpack --config ./export/webpack.config.export.js --progress",
    "export-run": "NODE_PATH=$NODE_PATH:../src NODE_ENV=production node --harmony ./export/serverManager.js --config ./export/webpack.config.dev"
  },
  "devDependencies": {
    "babel-core": "6.5.2",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "bootstrap-sass": "3.3.7",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "color-convert": "1.4.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.22.0",
    "ignore-loader": "0.1.1",
    "istanbul": "1.0.0-alpha.2",
    "jscs": "2.10.1",
    "jsdom": "9.2.1",
    "memory-fs": "0.3.0",
    "mocha": "2.5.3",
    "nock": "8.0.0",
    "node-sass": "3.4.2",
    "postcss-loader": "1.1.1",
    "postcss-smart-import": "0.5.1",
    "precss": "1.4.0",
    "react-addons-test-utils": "15.1.0",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.4",
    "redux-mock-store": "1.1.0",
    "sass-loader": "3.1.2",
    "selenium-webdriver": "3.0.0-beta-2",
    "style-loader": "0.13.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-manifest-plugin": "1.0.1"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "handlebars": "4.0.5",
    "isomorphic-fetch": "2.2.1",
    "less": "2.6.1",
    "less-loader": "2.2.2",
    "lodash": "4.5.0",
    "path": "0.12.7",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.0.0",
    "redux": "3.5.2",
    "redux-form": "5.3.1",
    "redux-thunk": "2.0.1"
  }
}
